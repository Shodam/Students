@model IEnumerable<WebApplication.Models.Student>

@{
    ViewData["Title"] = "All Students";
    Layout = "_Layout";
}
<div class="Intro-of-table">
    <h1>Student List Results</h1>
    <p>List of students registered in the database.</p>
</div>
<table class="table">
    <tr>
        <th>Student ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>GPA</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayTextFor(modelItem => item.StudentId)
            </td>
            <td>
                @Html.DisplayTextFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayTextFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayTextFor(modelItem => item.Gpa)
            </td>
        </tr>
    }
</table>